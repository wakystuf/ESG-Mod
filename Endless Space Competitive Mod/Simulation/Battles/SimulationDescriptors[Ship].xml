  <SimulationDescriptor Name="ShipRoleObliterator"         Type="ShipRole">
    <Property Name="TurnsSinceObliteratorShot"              BaseValue="0" IsSealed="true"/>
    <Property Name="NetTurnsSinceObliteratorShot"           BaseValue="1"/>

    <Property Name="BaseTurnsBeforeFreeObliteratorShot"     BaseValue="20"/>
    <Property Name="BonusTurnsBeforeFreeObliteratorShot"    BaseValue="0" MinValue="Negative"/>
    <Property Name="TurnsBeforeNextFreeObliteratorShot"     BaseValue="0"/>
    <Property Name="ObliteratorShotCost"                    BaseValue="0"/>

    <Property Name="ObliteratorShotsThisTurn"               BaseValue="0" IsSealed="true"/>
    <Property Name="NetObliteratorShotsThisTurn"            BaseValue="-100" MinValue="Negative"/>
    <Property Name="ExcessiveObliteratorShotsThisTurn"      BaseValue="-1" MinValue="0"/>

    <!-- Turns before next free shot = turns for free shot - turns since last shot. That's pretty easy. -->
    <BinaryModifier TargetProperty="TurnsBeforeNextFreeObliteratorShot" Operation="Addition" Left="$(BaseTurnsBeforeFreeObliteratorShot)" BinaryOperation="Subtraction" Right="$(TurnsSinceObliteratorShot)"/>
    <Modifier TargetProperty="TurnsBeforeNextFreeObliteratorShot" Operation="Addition" Value="$(BonusTurnsBeforeFreeObliteratorShot)" />

    <!-- Cost = turns before free Obliterator shot x baseCost x multiplier from shots this turn. -->
    <BinaryModifier TargetProperty="ObliteratorShotCost" Operation="Addition" Left="$(TurnsBeforeNextFreeObliteratorShot)" BinaryOperation="Multiplication" Right="6"/>
    <Modifier TargetProperty="ObliteratorShotCost" Operation="Percent" Value="$(ExcessiveObliteratorShotsThisTurn)"/>

    <Modifier TargetProperty="ExcessiveObliteratorShotsThisTurn" Operation="Addition" Value="$(ObliteratorShotsThisTurn)"/>

    <Modifier TargetProperty="ObliteratorCount"   Operation="Addition"  Value="1" Path="../ClassEmpire"         TooltipHidden="true"/>
    <Modifier TargetProperty="AntiVisionStrengthForModule" Operation="Force"     Value="0" Path="../ClassGarrisonFleet"  TooltipHidden="true" Priority="0"/>
  </SimulationDescriptor>


