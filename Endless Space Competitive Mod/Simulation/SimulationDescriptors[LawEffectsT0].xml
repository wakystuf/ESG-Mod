		<Modifier TargetProperty="OpponentRecyclingManpowerPerCPFlotilla"       Operation="Addition" Value="20"                                                                            Path="../ClassEmpire,FactionTraitDefensiveEconomy"             TooltipHiddenIfPathInvalid="true" TooltipOverride="%FactionTraitDefensiveEconomyMilitarists"/>
		<BinaryModifier TargetProperty="OpponentRecyclingManpowerPerCPFlotilla" Operation="Addition" Left="20" BinaryOperation="Multiplication" Right="$(ScavengersCodeAnywhereDisabled)"  Path="../ClassEmpire,FactionTraitDefensiveEconomy/ClassGarrisonFleet,FleetInfluenceStateOwner/ClassShip"    TooltipHidden="true" SearchValueFromPath="true"/>
    	<BinaryModifier TargetProperty="OpponentRecyclingManpowerPerCPFlotilla" Operation="Addition" Left="20" BinaryOperation="Multiplication" Right="$(ScavengersCodeAnywhereDisabled)"  Path="../ClassEmpire,FactionTraitDefensiveEconomy/ClassGarrisonFleet,FleetInfluenceStateFriendly/ClassShip" TooltipHidden="true" SearchValueFromPath="true"/>
    	<BinaryModifier TargetProperty="OpponentRecyclingManpowerPerCPFlotilla" Operation="Addition" Left="20" BinaryOperation="Multiplication" Right="$(ScavengersCodeAnywhere)"          Path="../ClassEmpire,FactionTraitDefensiveEconomy/ClassGarrisonFleet/ClassShip"                             TooltipHidden="true" SearchValueFromPath="true"/>
