<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../../Schemas/Amplitude.Unity.Simulation.SimulationDescriptor.xsd">
  <!-- Garden of Eden -->
  <SimulationDescriptor Name="PlanetAnomaly01Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="RawMaximumPopulation"         Value="1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawFood"                Value="1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawIndustry"            Value="1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawDust"                  Value="1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience"             Value="1" Operation="Addition" Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="2" BinaryOperation="Multiplication" Right="$(Population)"      />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="2" TooltipHidden="true" />
  </SimulationDescriptor>

  <!-- Dust Ruins -->
  <SimulationDescriptor Name="PlanetAnomaly02Regular" Type="PlanetAnomaly" >
    <Modifier           TargetProperty="PlanetRawDust" Value="3" Operation="Addition" Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="2" BinaryOperation="Multiplication" Right="$(Population)"      />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="2" TooltipHidden="true" />
  </SimulationDescriptor>

  <!-- Hadopelagic Life -->
  <SimulationDescriptor Name="PlanetAnomaly06Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawFood"     Value="1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience"  Value="2" Operation="Addition" Path="ClassPlanet"/>
  </SimulationDescriptor>

  <!-- Antonov Ring -->
  <SimulationDescriptor Name="PlanetAnomaly07Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawDust"    Value="2" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience" Value="1" Operation="Addition" Path="ClassPlanet"/>
  </SimulationDescriptor>

  <!-- Huygens Rings -->
  <SimulationDescriptor Name="PlanetAnomaly08Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawDust" Value="1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience" Value="2" Operation="Addition" Path="ClassPlanet"/>
  </SimulationDescriptor>

  <!-- Ice-10 -->
  <SimulationDescriptor Name="PlanetAnomaly10Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawScience"         Value="2"  Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawFood"            Value="-2" Operation="Addition" Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="-2" BinaryOperation="Multiplication" Right="$(Population)"      />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="-2" TooltipHidden="true" />
  </SimulationDescriptor>

  <!-- Permament Monsoon -->
  <SimulationDescriptor Name="PlanetAnomaly11Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawFood"        Value="2"   Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawIndustry"    Value="-2"  Operation="Addition" Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="-2" BinaryOperation="Multiplication" Right="$(Population)"      />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="-2" TooltipHidden="true" />
  </SimulationDescriptor>

  <!-- Metallic Waters -->
  <SimulationDescriptor Name="PlanetAnomaly12Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawIndustry" Value="1"  Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawFood"     Value="-1" Operation="Addition" Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="-2" BinaryOperation="Multiplication" Right="$(Population)"      />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="-2" TooltipHidden="true" />
  </SimulationDescriptor>

  <!-- Aurora Waves -->
  <SimulationDescriptor Name="PlanetAnomaly13Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawScience" Value="2" Operation="Subtraction" Path="ClassPlanet" Priority="1"/>
    <Modifier TargetProperty="PlanetRawDust"    Value="2"  Operation="Addition" Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="1" BinaryOperation="Multiplication" Right="$(Population)"      />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="1" TooltipHidden="true" />
  </SimulationDescriptor>

  <!-- Dense Atmosphere -->
  <SimulationDescriptor Name="PlanetAnomaly14Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawFood"            Value="2" Operation="Addition" Path="ClassPlanet"/>
  </SimulationDescriptor>

  <!-- Psychoactive Air -->
  <SimulationDescriptor Name="PlanetAnomaly15Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawScience" Value="-1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawDust"    Value="1"  Operation="Addition" Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="1" BinaryOperation="Multiplication" Right="$(Population)"      />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="1" TooltipHidden="true" />
  </SimulationDescriptor>

  <!-- Geothermic Activity -->
  <SimulationDescriptor Name="PlanetAnomaly16Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawIndustry" Value="1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience" Value="-1" Operation="Addition" Path="ClassPlanet"/>
  </SimulationDescriptor>

  <!-- Meteor Strikes -->
  <SimulationDescriptor Name="PlanetAnomaly19Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawIndustry" Value="1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience" Value="-1" Operation="Addition" Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="-2" BinaryOperation="Multiplication" Right="$(Population)"      />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="-2" TooltipHidden="true" />
  </SimulationDescriptor>

  <!-- Polar Tempests -->
  <SimulationDescriptor Name="PlanetAnomaly20Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawFood" Value="-1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawIndustry" Value="-1" Operation="Addition" Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="-2" BinaryOperation="Multiplication" Right="$(Population)"      />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="-2" TooltipHidden="true" />
  </SimulationDescriptor>

  <!-- Strong Magnetic Field -->
  <SimulationDescriptor Name="PlanetAnomaly21Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawIndustry" Value="-1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience" Value="-1" Operation="Addition" Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="-2" BinaryOperation="Multiplication" Right="$(Population)"      />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="-2" TooltipHidden="true" />
  </SimulationDescriptor>

  <!-- Kessler Syndrome -->
  <SimulationDescriptor Name="PlanetAnomaly22Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawIndustry" Value="-1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawDust" Value="-1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience" Value="-2" Operation="Addition" Path="ClassPlanet"/>
  </SimulationDescriptor>

  <!-- Dust Lode -->
  <SimulationDescriptor Name="PlanetAnomaly30Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawDust"                    Value="2"   Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience"                 Value="2"   Operation="Addition" Path="ClassPlanet"/>
  </SimulationDescriptor>

  <!-- Molten Spring   -->
  <SimulationDescriptor Name="PlanetAnomaly38Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawIndustry"                Value="3"       Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawDust"                    Value="2"       Operation="Addition" Path="ClassPlanet"/>
  </SimulationDescriptor>

  <!-- High-Energy Plasmasphere -->
  <SimulationDescriptor Name="PlanetAnomaly48Regular" Type="PlanetAnomaly" >
    <Modifier           TargetProperty="PlanetRawIndustry"      Operation="Addition" Value="1"      Path="ClassPlanet"/>
    <Modifier           TargetProperty="PlanetRawScience"       Operation="Addition" Value="2"      Path="ClassPlanet"/>
    <BinaryModifier     TargetProperty="MaximumSystemManpower"  Operation="Addition" Left="10"      BinaryOperation="Multiplication" Right="$(Population)" Path="./ClassColonizedStarSystem" TooltipHidden="true"    />
    <BinaryModifier     TargetProperty="MaximumSystemManpower"  Operation="Addition" Left="10"      BinaryOperation="Multiplication" Right="$(Population)" Path="./ClassColonizedStarSystem/ClassColonizedPlanet" />
  </SimulationDescriptor>

  <!--  Dust Halo -->
  <SimulationDescriptor Name="PlanetAnomaly49Regular" Type="PlanetAnomaly" >
    <Modifier TargetProperty="PlanetRawDust"                Value="4" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience"             Value="1" Operation="Addition" Path="ClassPlanet"/>
    <Modifier TargetProperty="PlanetRawScience"             Value="1" Operation="Addition" Path="ClassPlanet"/>

    <BinaryModifier     TargetProperty="Happiness"          Operation="Addition" Left="2" BinaryOperation="Multiplication" Right="$(Population)" TooltipHidden="true"     />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="2"  />
  </SimulationDescriptor>
  
  <!-- Wonders -->
  <!-- Dust Nebula -->
  <SimulationDescriptor Name="PlanetAnomalyNaturalWonder1Regular" Type="PlanetAnomaly">
    <Modifier   TargetProperty="PlanetRawDust"  Value="2"    Operation="Addition" Path="ClassPlanet"/>
    <Modifier   TargetProperty="SystemMoney"    Value="50"   Operation="Addition" Path="./ClassStarSystem"/>
  </SimulationDescriptor>

  <!-- Seventeen Thousand Islands Bridge-->
  <SimulationDescriptor Name="PlanetAnomalyNaturalWonder2Regular" Type="PlanetAnomaly">
    <Modifier       TargetProperty="RawMaximumPopulation"   Value="2"           Operation="Addition" Path="ClassPlanet" />
    <Modifier       TargetProperty="PlanetRawFIDSI"         Value="1"           Operation="Addition" Path="ClassPlanet"/>
  </SimulationDescriptor>

  <!-- Fallen Gardens -->
  <SimulationDescriptor Name="PlanetAnomalyNaturalWonder3Regular" Type="PlanetAnomaly">
    <Modifier   TargetProperty="PlanetRawIndustry"              Value="2"        Operation="Addition" Path="ClassPlanet"/>
    <Modifier   TargetProperty="ShipProductionCostReduction"    Value="-0.05"    Operation="Addition" Path="./ClassColonizedStarSystem"/>
  </SimulationDescriptor>

  <!-- Platform of Ys -->
  <SimulationDescriptor Name="PlanetAnomalyNaturalWonder4Regular" Type="PlanetAnomaly">
    <Modifier       TargetProperty="RawMaximumPopulation"   Value="2"           Operation="Addition" Path="ClassPlanet" />
    <Modifier       TargetProperty="MaximumSystemManpower"  Value="500"         Operation="Addition" Path="./ClassColonizedStarSystem"/>
  </SimulationDescriptor>

  <!-- Tree of World -->
  <SimulationDescriptor Name="PlanetAnomalyNaturalWonder5Regular" Type="PlanetAnomaly">
    <Modifier    TargetProperty="PlanetRawFood"      Value="2"           Operation="Addition" Path="ClassPlanet" />
    <Modifier    TargetProperty="SystemGrowth"       Value="20"          Operation="Addition" Path="./ClassColonizedStarSystem"/>
    <BinaryModifier     TargetProperty="Happiness"   Operation="Addition" Left="6" BinaryOperation="Multiplication" Right="$(Population)" Path="ClassPlanet" />
    <Modifier           TargetProperty="HappinessRaw"       Operation="Addition" Value="6" TooltipHidden="true" Path="ClassPlanet" />
  </SimulationDescriptor>

  <!-- Tunneled Slipgates -->
  <SimulationDescriptor Name="PlanetAnomalyNaturalWonder6Regular" Type="PlanetAnomaly">
    <Modifier    TargetProperty="BlockadeImpactFactor"               Value="0"           Operation="Force"          Path="./ClassColonizedStarSystem,BlockadedColonizedStarSystem" Priority="-1"/>
    <Modifier    TargetProperty="BlockadeImpactFactor"               Value="0"           Operation="Force"          Path="./ClassColonizedStarSystem,BesiegedColonizedStarSystem" Priority="-1" TooltipHidden="true"/>
    <Modifier    TargetProperty="TradingRouteIncomeModifier"         Value="1"           Operation="Addition"       Path="./ClassColonizedStarSystem" Priority="1"/>
  </SimulationDescriptor>
</Datatable>