<?xml version="1.0" encoding="utf-8" ?>
<Datatable xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="../Schemas/Amplitude.Unity.Simulation.SimulationDescriptor.xsd">

  <SimulationDescriptor Name="ClassColonizedPlanet" Type="ClassColonizedPlanet">
    <!-- ESCM -->
    <Property Name="LuxuryReworkEnabled"                BaseValue="0"/>
    <Property Name="LuxuryReworkDisabled"               BaseValue="0"/>
    
    <!-- ########################################################## -->
    <!-- ######################## PROPERTY ######################## -->
    <!-- ########################################################## -->
    <Property Name="OwnedPlanets"                           BaseValue="1" />


    <Property Name="PlanetPopulationFood"      MinValue="Negative" />
    <Property Name="PlanetPopulationIndustry"  MinValue="Negative" />
    <Property Name="PlanetPopulationDust"      MinValue="Negative" />
    <Property Name="PlanetPopulationScience"   MinValue="Negative" />
    <Property Name="PlanetPopulationPrestige"  MinValue="Negative" />

    <Property Name="PlanetDepletingPopulationFood"      MinValue="Negative" />
    <Property Name="PlanetDepletingPopulationIndustry"  MinValue="Negative" />
    <Property Name="PlanetDepletingPopulationDust"      MinValue="Negative" />
    <Property Name="PlanetDepletingPopulationScience"   MinValue="Negative" />
    <Property Name="PlanetDepletingPopulationPrestige"  MinValue="Negative" />

    <Property Name="PlanetParasitedPopulationFood"      MinValue="Negative" />
    <Property Name="PlanetParasitedPopulationIndustry"  MinValue="Negative" />
    <Property Name="PlanetParasitedPopulationDust"      MinValue="Negative" />
    <Property Name="PlanetParasitedPopulationScience"   MinValue="Negative" />
    <Property Name="PlanetParasitedPopulationPrestige"  MinValue="Negative" />

    <Property Name="DefaultPlanetFood"/>
    <Property Name="DefaultPlanetIndustry"/>
    <Property Name="DefaultPlanetDust"/>
    <Property Name="DefaultPlanetScience"/>
    <Property Name="DefaultPlanetPrestige"/>

    <Property Name="DefaultPlanetScienceMalus"/>
    <Property Name="DefaultPlanetScienceMalusFactor"/>
    
    <Property Name="PlanetRawFoodFromMiningProbes"/>
    <Property Name="PlanetRawIndustryFromMiningProbes"/>
    <Property Name="PlanetRawDustFromMiningProbes"/>
    <Property Name="PlanetRawScienceFromMiningProbes"/>
    <Property Name="PlanetHasBonusesFromMiningProbes"/>

    <Property Name="PlanetHeroFIDSIPercent"/>
    <Property Name="PlanetHeroFIDSIFlat"/>

    <Property Name="PlanetFIDSI"/>
    <Property Name="PlanetFIDSIDevelopment"/>

    <Property Name="PlanetFood"/>
    <Property Name="PlanetIndustry"/>
    <Property Name="PlanetDust"/>
    <Property Name="PlanetScience"/>
    <Property Name="PlanetPrestige"/>

    <!-- FIDSI generated by the Assigned Hero (feedback only) -->
    <Property Name="BonusHeroPlanetFood"        BaseValue="0"/>
    <Property Name="BonusHeroPlanetIndustry"    BaseValue="0"/>
    <Property Name="BonusHeroPlanetDust"        BaseValue="0"/>
    <Property Name="BonusHeroPlanetScience"     BaseValue="0"/>
    <Property Name="BonusHeroPlanetPrestige"    BaseValue="0"/>

    <Property Name="PlanetOutpostFood" MinValue="Negative"/>
    <Property Name="PlanetOutpostIndustry" MinValue="Negative"/>
    <Property Name="PlanetOutpostDust" MinValue="Negative"/>
    <Property Name="PlanetOutpostScience" MinValue="Negative"/>
    <Property Name="PlanetOutpostPrestige" MinValue="Negative"/>


    <Property Name="OverPopulationThreshold"                BaseValue="0" />
    <Property Name="PopulationPercentForOverPopulation"     BaseValue="0.7" />
    <Property Name="OverPopulationMalus"                    BaseValue="0"  MinValue="Negative" MaxValue="0"/>
    <Property Name="ParasitedPopulation"                    BaseValue="0"   />
    <Property Name="MaximumPopulationFromMothership"/>
    <Property Name="ShouldOverrideBaseMaximumPopulation" BaseValue="0" />

    <Property Name="PopulationCountPulsos" BaseValue="0" />
    <Property Name="PopulationCountRemnant" BaseValue="0" />
    <Property Name="PopulationCountSefaloros" BaseValue="0" />
    <Property Name="PopulationCountGalvrans" BaseValue="0" />
    <Property Name="PopulationCountHarmony" BaseValue="0" />

    <Property Name="PotentialPlanetRawPrestige" BaseValue="0" />
    <Property Name="PotentialPlanetRawScience" BaseValue="0" />
    <Property Name="PotentialPlanetRawFood" BaseValue="0" />
    <Property Name="PotentialPlanetRawHappiness" BaseValue="0" />
    <Property Name="PotentialPlanetRawHappinessIfDeposit" BaseValue="0"/>
    <Property Name="SuperPopulationCount" BaseValue="0"/>

    <Property Name="TemplePopulationCount" BaseValue="0"/>
    <Property Name="HasTemple" BaseValue="0"/>

    <!-- ########################################################## -->
    <!-- ######################## MODIFIER ######################## -->
    <!-- ########################################################## -->
    <Modifier TargetProperty="RawPopulationFood"         Operation="Addition" Value="$(PlanetRawFood)"        Path="ClassColonizedPlanet/ClassPopulationPlanet" TooltipHidden="true"/>
    <Modifier TargetProperty="RawPopulationIndustry"     Operation="Addition" Value="$(PlanetRawIndustry)"    Path="ClassColonizedPlanet/ClassPopulationPlanet" TooltipHidden="true"/>
    <Modifier TargetProperty="RawPopulationDust"         Operation="Addition" Value="$(PlanetRawDust)"        Path="ClassColonizedPlanet/ClassPopulationPlanet" TooltipHidden="true"/>
    <Modifier TargetProperty="RawPopulationScience"      Operation="Addition" Value="$(PlanetRawScience)"     Path="ClassColonizedPlanet/ClassPopulationPlanet" TooltipHidden="true"/>
    <Modifier TargetProperty="RawPopulationPrestige"     Operation="Addition" Value="$(PlanetRawPrestige)"    Path="ClassColonizedPlanet/ClassPopulationPlanet" TooltipHidden="true"/>

    <BinaryModifier TargetProperty="DefaultPlanetFood"         Operation="Addition" Left="$(PlanetRawFood)"       BinaryOperation="Multiplication"     Right="$(Population)"    TooltipHidden="true" />
    <BinaryModifier TargetProperty="DefaultPlanetIndustry"     Operation="Addition" Left="$(PlanetRawIndustry)"   BinaryOperation="Multiplication"     Right="$(Population)"    TooltipHidden="true" />
    <BinaryModifier TargetProperty="DefaultPlanetDust"         Operation="Addition" Left="$(PlanetRawDust)"       BinaryOperation="Multiplication"     Right="$(Population)"    TooltipHidden="true" />
    <BinaryModifier TargetProperty="DefaultPlanetScience"      Operation="Addition" Left="$(PlanetRawScience)"    BinaryOperation="Multiplication"     Right="$(Population)"    TooltipHidden="true" />
    <BinaryModifier TargetProperty="DefaultPlanetScienceMalus" Operation="Addition" Left="$(DefaultPlanetScience)" BinaryOperation="Multiplication"     Right="$(DefaultPlanetScienceMalusFactor)" TooltipHidden="true" />
    <BinaryModifier TargetProperty="DefaultPlanetPrestige"     Operation="Addition" Left="$(PlanetRawPrestige)"   BinaryOperation="Multiplication"     Right="$(Population)"    TooltipHidden="true" />

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(PlanetFIDSI)"  TooltipHidden="true"    />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(PlanetFIDSI)"  TooltipHidden="true"      />
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(PlanetFIDSI)"  TooltipHidden="true"    />
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(PlanetFIDSI)"  TooltipHidden="true"      />
    <Modifier TargetProperty="PlanetPrestige"         Operation="Addition" Value="$(PlanetFIDSI)"  TooltipHidden="true"      />

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(DefaultPlanetFood)"         TooltipHidden="false" />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(DefaultPlanetIndustry)"     TooltipHidden="false" />
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(DefaultPlanetDust)"         TooltipHidden="false" />
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(DefaultPlanetScience)"      TooltipHidden="false" />
    <Modifier TargetProperty="PlanetScience"          Operation="Subtraction" Value="$(DefaultPlanetScienceMalus)" TooltipHidden="false"/>
    <Modifier TargetProperty="PlanetPrestige"         Operation="Addition" Value="$(DefaultPlanetPrestige)"     TooltipHidden="false" />

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(PlanetPopulationFood)"      TooltipHidden="true"    />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(PlanetPopulationIndustry)"  TooltipHidden="true" />
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(PlanetPopulationDust)"      TooltipHidden="true" />
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(PlanetPopulationScience)"   TooltipHidden="true" />
    <Modifier TargetProperty="PlanetPrestige"         Operation="Addition" Value="$(PlanetPopulationPrestige)"  TooltipHidden="true" />

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(PlanetDepletingPopulationFood)"     TooltipHidden="false"    />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(PlanetDepletingPopulationIndustry)" TooltipHidden="false"    />
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(PlanetDepletingPopulationDust)"     TooltipHidden="false"    />
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(PlanetDepletingPopulationScience)"  TooltipHidden="false"    />
    <Modifier TargetProperty="PlanetPrestige"         Operation="Addition" Value="$(PlanetDepletingPopulationPrestige)" TooltipHidden="false"    />

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(PlanetParasitedPopulationFood)"     TooltipHidden="false"   />
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(PlanetParasitedPopulationIndustry)" TooltipHidden="false"    />
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(PlanetParasitedPopulationDust)"     TooltipHidden="false"    />
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(PlanetParasitedPopulationScience)"  TooltipHidden="false"    />
    <Modifier TargetProperty="PlanetPrestige"         Operation="Addition" Value="$(PlanetParasitedPopulationPrestige)" TooltipHidden="false"    />


    <Modifier TargetProperty="PlanetFood"             Operation="Percent" Value="$(PlanetHeroFIDSIPercent)"    TooltipHidden="false"/>
    <Modifier TargetProperty="PlanetIndustry"         Operation="Percent" Value="$(PlanetHeroFIDSIPercent)"    TooltipHidden="false"/>
    <Modifier TargetProperty="PlanetDust"             Operation="Percent" Value="$(PlanetHeroFIDSIPercent)"    TooltipHidden="false"/>
    <Modifier TargetProperty="PlanetScience"          Operation="Percent" Value="$(PlanetHeroFIDSIPercent)"    TooltipHidden="false"/>
    <Modifier TargetProperty="PlanetPrestige"         Operation="Percent" Value="$(PlanetHeroFIDSIPercent)"    TooltipHidden="false"/>

    <Modifier TargetProperty="PlanetFood"             Operation="Addition" Value="$(PlanetHeroFIDSIFlat)"    TooltipHidden="false"/>
    <Modifier TargetProperty="PlanetIndustry"         Operation="Addition" Value="$(PlanetHeroFIDSIFlat)"    TooltipHidden="false"/>
    <Modifier TargetProperty="PlanetDust"             Operation="Addition" Value="$(PlanetHeroFIDSIFlat)"    TooltipHidden="false"/>
    <Modifier TargetProperty="PlanetScience"          Operation="Addition" Value="$(PlanetHeroFIDSIFlat)"    TooltipHidden="false"/>
    <Modifier TargetProperty="PlanetPrestige"         Operation="Addition" Value="$(PlanetHeroFIDSIFlat)"    TooltipHidden="false"/>

    <Modifier TargetProperty="PlanetRawFood"             Operation="Addition" Value="$(PlanetRawFoodFromMiningProbes)"       TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawIndustry"         Operation="Addition" Value="$(PlanetRawIndustryFromMiningProbes)"   TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawDust"             Operation="Addition" Value="$(PlanetRawDustFromMiningProbes)"       TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetRawScience"          Operation="Addition" Value="$(PlanetRawScienceFromMiningProbes)"    TooltipHidden="true"/>

    <Modifier TargetProperty="PlanetHasBonusesFromMiningProbes" Operation="Addition" Value="$(PlanetRawFoodFromMiningProbes)"       TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetHasBonusesFromMiningProbes" Operation="Addition" Value="$(PlanetRawIndustryFromMiningProbes)"   TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetHasBonusesFromMiningProbes" Operation="Addition" Value="$(PlanetRawDustFromMiningProbes)"       TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetHasBonusesFromMiningProbes" Operation="Addition" Value="$(PlanetRawScienceFromMiningProbes)"    TooltipHidden="true"/>

    <Modifier TargetProperty="PlanetFoodReceiver"             Operation="Addition" Value="$(PlanetFood)"     Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetIndustryReceiver"         Operation="Addition" Value="$(PlanetIndustry)" Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetDustReceiver"             Operation="Addition" Value="$(PlanetDust)"     Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetScienceReceiver"          Operation="Addition" Value="$(PlanetScience)"  Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="PlanetPrestigeReceiver"         Operation="Addition" Value="$(PlanetPrestige)" Path="./ClassColonizedStarSystem" TooltipHidden="true"/>

    <Modifier TargetProperty="BonusHeroSystemFood"      Operation="Addition" Value="$(BonusHeroPlanetFood)"     Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="BonusHeroSystemIndustry"  Operation="Addition" Value="$(BonusHeroPlanetIndustry)" Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="BonusHeroSystemDust"      Operation="Addition" Value="$(BonusHeroPlanetDust)"     Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="BonusHeroSystemScience"   Operation="Addition" Value="$(BonusHeroPlanetScience)"  Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
    <Modifier TargetProperty="BonusHeroSystemPrestige"  Operation="Addition" Value="$(BonusHeroPlanetPrestige)" Path="./ClassColonizedStarSystem" TooltipHidden="true"/>

    <Modifier TargetProperty="MaximumPopulation"      Operation="Addition" Value="$(MaximumPopulation)" Path="ClassColonizedPlanet/../ClassStarSystem" TooltipHidden="true"/>

    <BinaryModifier TargetProperty="OverPopulationThreshold"    Operation="Addition"             Left="$(MaximumPopulation)"       BinaryOperation="Multiplication" Right="$(PopulationPercentForOverPopulation)" TooltipHidden="true"/>
    <BinaryModifier TargetProperty="OverPopulationMalus"        Operation="Addition"             Left="$(OverPopulationThreshold)" BinaryOperation="Subtraction"    Right="$(Population)" TooltipHidden="true"/>
    <BinaryModifier TargetProperty="OverPopulationPenalties"    Operation="Addition"             Left="$(OverPopulationMalus)"     BinaryOperation="Multiplication" Right="3.75" Path="../ClassColonizedStarSystem" TooltipHidden="true"/>

    <Modifier TargetProperty="PlanetHasTemple" Operation="Force" Value="$(HasTemple)" Path="ClassPlanet/ClassPopulationPlanet" TooltipHidden="true"/>

      <!-- For mothership -->
    <BinaryModifier TargetProperty="ShouldOverrideBaseMaximumPopulation" Operation="Addition" Left="$(MaximumPopulationFromMothership)" BinaryOperation="Subtraction"  Right="$(RawMaximumPopulation)"/>
    <Modifier       TargetProperty="BaseMaximumPopulation"               Operation="Addition" Value="$(ShouldOverrideBaseMaximumPopulation)"/>

    <Modifier TargetProperty="Happiness"      Operation="Addition" Value="$(PotentialPlanetRawHappinessIfDeposit)" Path="./ClassColonizedStarSystem" TooltipHidden="true"/>
  </SimulationDescriptor>

</Datatable>