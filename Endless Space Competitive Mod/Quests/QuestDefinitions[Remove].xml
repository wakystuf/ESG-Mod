  <!-- AI Personality -->
  <QuestDefinition Name="RemovePersonalityTraitsAfterAssignment" Category="Removal" SubCategory="AITrait" TriggeringProbability="1" MinimumTurn="0">
    <Tags>Hidden,BeginTurn</Tags>
    <QuestContextSolo/>
    <RepetitionRules NumberOfOccurrencesPerEmpire="1"/>
